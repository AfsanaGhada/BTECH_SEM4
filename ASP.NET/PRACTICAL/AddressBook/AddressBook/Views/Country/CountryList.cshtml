


<main id="main" class="main">

    <div class="pagetitle">
        <h1>Country</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Country</a></li>
                <li class="breadcrumb-item">Tables</li>
                <li class="breadcrumb-item active">Data</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">

                <div class="card">
                    <div class="card-body">
                        <div>
                            <h5 class="card-title">Search User</h5>

                            <form class="row g-3">
                                <div class="col-md-4">
                                    <label for="CountryName" class="form-label">CountryName</label>

                                    <input type="text" class="form-control  border border-black" placeholder="Enter Country Name">
                                </div>
                                <div class="col-md-4">
                                    <label for="CountryCode" class="form-label">CountryCode</label>

                                    <input type="text" class="form-control  border border-black" placeholder="Enter CountryCode">
                                </div>
                               
                                <div class="text-start">
                                    <button type="submit" class="btn btn-success">Search</button>
                                    <button type="reset" class="btn btn-danger">Reset</button>
                                </div>
                            </form><!-- End No Labels Form -->
                        </div>
                    </div>
                </div>
            </div>
    </section>


    <section class="section">
        <div class="row">
            <div class="col-lg-12">

                <div class="card">
                    <div class="card-body">
                        <a class="btn btn-success float-end mb-2 mt-3" asp-area="" asp-controller="Country" asp-action="CountryForm">Add State</a>

                        <!-- Table with stripped rows -->
                        <table class="table datatable">
                            <h2 class="mt-3">Country list</h2>


                            <thead>
                                <tr class="table-dark">
                                    @* <th>CountryID </th> *@
                                    <th>CountryName</th>
                                    <th>CountryCode</th>
                                    @* <th>CreationDate</th> *@

                                </tr>
                            </thead>

                            <tbody>
                                <tr>
                                    @* <td>1</td> *@
                                    <td>United States</td>
                                    <td>US</td>
                                    @* <td>1776-07-04</td> *@
                                </tr>
                                <tr>
                                    @* <td>2</td> *@
                                    <td>Canada</td>
                                    <td>CA</td>
                                    @* <td>1867-07-01</td> *@
                                </tr>
                                <tr>
                                    @* <td>3</td> *@
                                    <td>Germany</td>
                                    <td>DE</td>
                                    @* <td>1871-01-18</td> *@
                                </tr>
                                <tr>
                                    @* <td>4</td> *@
                                    <td>India</td>
                                    <td>IN</td>
                                    @* <td>1947-08-15</td> *@
                                </tr>
                                <tr>
                                    @* <td>5</td> *@
                                    <td>Australia</td>
                                    <td>AU</td>
                                    @* <td>1901-01-01</td> *@
                                </tr>
                            </tbody>
                        </table>
                        <!-- End Table with stripped rows -->

                    </div>
                </div>

            </div>
        </div>
    </section>

</main><!-- End #main -->
